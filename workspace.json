{
  "version": 2,
  "projects": {
    "access-control": {
      "root": "modules/access-control",
      "projectType": "library"
    },
    "authentication-portal": {
      "root": "apps/authentication-portal",
      "projectType": "application"
    },
    "console": {
      "projectType": "application",
      "generators": {},
      "root": "apps/console",
      "sourceRoot": "apps/console/src",
      "prefix": "@wso2is",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [
            "apps/build/console"
          ],
          "options": {
            "command": "npm run build",
            "cwd": "apps/console"
          }
        },
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [
            "apps/build/console"
          ],
          "options": {
            "command": "npm run start",
            "cwd": "apps/console"
          }
        }
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "core": {
      "projectType": "library",
      "generators": {},
      "root": "modules/core",
      "sourceRoot": "modules/core/src",
      "prefix": "@wso2is",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [
            "modules/core/dist"
          ],
          "options": {
            "tsconfig": "modules/core/tsconfig.json",
            "command": "npm run compile",
            "cwd": "modules/core"
          }
        }
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "form": {
      "projectType": "library",
      "generators": {},
      "root": "modules/form",
      "sourceRoot": "modules/form/src",
      "prefix": "@wso2is",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [
            "modules/form/dist"
          ],
          "options": {
            "command": "npm run build",
            "cwd": "modules/form"
          }
        }
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "forms": {
      "projectType": "library",
      "generators": {},
      "root": "modules/forms",
      "sourceRoot": "modules/forms/src",
      "prefix": "@wso2is",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [
            "modules/forms/dist"
          ],
          "options": {
            "command": "npm run build",
            "cwd": "modules/forms"
          }
        }
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "i18n": {
      "root": "modules/i18n",
      "projectType": "library"
    },
    "myaccount": {
      "projectType": "application",
      "generators": {},
      "root": "apps/myaccount",
      "sourceRoot": "apps/myaccount/src",
      "prefix": "@wso2is",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [
            "apps/build/myaccount"
          ],
          "options": {
            "command": "npm run build",
            "cwd": "apps/myaccount",
            "outputPath": "apps/myaccount/build/myaccount"
          }
        },
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "command": "npm run start",
            "cwd": "apps/myaccount",
            "outputPath": "apps/myaccount/build/myaccount"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/myaccount/**/*.{ts,tsx,js,jsx}"
            ],
            "eslintConfig": "apps/myaccount/.eslintrc.js"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "apps/build/myaccount"
          ],
          "options": {
            "command": "npm run start",
            "cwd": "apps/myaccount"
          }
        }
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "react-components": {
      "projectType": "library",
      "generators": {},
      "root": "modules/react-components",
      "sourceRoot": "modules/react-components/src",
      "prefix": "@wso2is",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [
            "modules/react-components/dist"
          ],
          "options": {
            "command": "npm run build",
            "cwd": "modules/react-components"
          }
        }
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "recovery-portal": {
      "root": "apps/recovery-portal",
      "projectType": "application"
    },
    "sms-otp-authentication-portal": {
      "root": "apps/sms-otp-authentication-portal",
      "projectType": "application"
    },
    "tests": {
      "root": "tests",
      "projectType": "library"
    },
    "theme": {
      "root": "modules/theme",
      "projectType": "library"
    },
    "theme-component": {
      "root": "components/theme-component",
      "projectType": "library"
    },
    "validation": {
      "root": "modules/validation",
      "projectType": "library"
    },
    "x509-certificate-authentication-portal": {
      "root": "apps/x509-certificate-authentication-portal",
      "projectType": "application"
    }
  }
}
