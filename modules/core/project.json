{
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "generators": {},
    "root": "modules/core",
    "sourceRoot": "modules/core/src",
    "prefix": "@wso2is",
    "targets": {
        "build": {
            "executor": "@nrwl/workspace:run-commands",
            "outputs": [
                "{options.outputPath}"
            ],
            "options": {
                "outputPath": "modules/core/dist",
                "tsconfig": "modules/core/tsconfig.json",
                "command": "pnpm build",
                "cwd": "modules/core"
            }
        },
        "lint": {
            "executor": "@nrwl/workspace:run-commands",
            "options": {
                "command": "pnpm lint",
                "cwd": "modules/core",
                "parallel": false
            }
        },
        "test": {
            "executor": "@nrwl/workspace:run-commands",
            "options": {
                "command": "pnpm test",
                "cwd": "modules/core",
                "parallel": false
            }
        }
    }
}
